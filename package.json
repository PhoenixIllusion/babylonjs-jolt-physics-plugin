{
  "name": "@phoenixillusion/babylonjs-jolt-plugin",
  "private": true,
  "version": "1.3.0",
  "type": "module",
  "scripts": {
    "build": "mkdir -p dist && cp src/jolt-import.js ./dist/. && tsc",
    "git-pages": "sh build-git-pages.sh",
